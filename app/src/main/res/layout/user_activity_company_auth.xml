            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/bg_white">

                <TextView
                    android:id="@+id/tvIdCardtr"
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="身份证号"
                    android:textColor="@color/t_black"
                    android:textSize="@dimen/sp_mid" />

                <EditText
                    android:id="@+id/edtIdCard"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="36dp"
                    android:layout_toRightOf="@id/tvIdCardtr"
                    android:background="@null"
                    android:hint="请输入"
                    android:textColorHint="@color/t_gray"
                    android:inputType="number"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="@color/t_black"
                    android:textSize="@dimen/sp_mid" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_alignParentBottom="true"
                    android:background="@color/bg_gray" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="营业执照副本照片"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_15sp"
                    android:layout_marginLeft="@dimen/icon_size_20dp"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="@dimen/sp_12"
                    android:textColor="#999999"
                    android:text="上传营业执照照片"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="250dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/bg_round_gray"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:id="@+id/imgLicense"
                        android:layout_width="200dp"
                        android:layout_height="110dp"
                        android:src="@mipmap/business_license"/>
                    <Button
                        android:id="@+id/btnLicense"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:text="上传营业执照"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/white"
                        android:background="@drawable/bg_red_rectangle"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
