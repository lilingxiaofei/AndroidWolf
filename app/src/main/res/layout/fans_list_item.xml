<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    android:minHeight="56dp"
    android:orientation="horizontal"
    android:background="@color/bg_white"
    tools:context="com.chunlangjiu.app.fans.activity.FansHomeActivity">
        <TextView android:id="@+id/tv_fans_name"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_14sp"
            android:textColor="@color/black"
            android:gravity="center"
            />
        <TextView  android:id="@+id/tv_fans_phone"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_14sp"
            android:textColor="@color/black"
            android:gravity="center"
            />
        <TextView android:id="@+id/tv_register_time"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_14sp"
            android:textColor="@color/black"
            android:gravity="center"
            />
        <TextView android:id="@+id/tv_money"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_14sp"
            android:textColor="@color/t_red"
            android:gravity="center"/>
</LinearLayout>
